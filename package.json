{
    "name": "Hypha",
    "version": "1.0.0",
    "description": "The node.js requirements to build this project.",
    "author": "Fredrik Jonsson",
    "license": "GPL-2.0",
    "browserslist": [
        "> 3%",
        "Firefox ESR",
        "last 2 versions"
    ],
    "dependencies": {
        "@date-io/date-fns": "^1.3.13",
        "@material-ui/core": "^4.11.4",

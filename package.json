        "@material-ui/pickers": "^3.3.10",
        "@rooks/use-interval": "^3.0.1",
        "@svgr/webpack": "^6.2.1",
        "@tinymce/tinymce-react": "^3.12.6",
        "connected-react-router": "^6.9.1",
        "core-js": "^3.15.2",
        "date-fns": "^2.22.1",
        "del": "^4.1.1",
        "detect-node": "^2.1.0",
        "humps": "^2.0.1",
        "invariant": "^2.2.4",
        "js-cookie": "^2.2.0",
        "lodash.isequal": "^4.5.0",
        "lodash.pick": "^4.4.0",
        "prop-types": "^15.7.2",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-modal": "^3.14.3",
        "react-redux": "^7.2.4",
        "react-router-dom": "^5.2.0",
        "react-rte": "^0.16.3",
        "react-transition-group": "^4.4.2",
        "react-window-size-listener": "^1.2.3",
        "redux": "^4.1.0",
        "redux-logger": "^3.0.6",
        "redux-mock-store": "^1.5.4",
        "redux-saga": "^1.1.3",
